{
    "name": "zablose/rap",
    "description": "RaP - Roles and Permissions",
    "keywords": [
        "roles",
        "permissions",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergejs Zablockis",
            "email": "zablose@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "laravel/framework": "^12.19.3",
        "laravel/tinker": "^2.10.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.1",
        "phpunit/phpunit": "^11.5.24"
    },
    "autoload": {
        "psr-4": {
            "Zablose\\Rap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "laravel/app/",
            "Tests\\": "laravel/tests/",
            "Database\\Factories\\": "laravel/database/factories/"
        }
    }
}
