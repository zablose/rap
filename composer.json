{
    "name": "zablose/rap",
    "description": "RaP - Roles and Permissions",
    "keywords": [
        "roles",
        "permissions",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergejs Zablockis",
            "email": "zablose@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/database": "^8.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5.1",
        "fideloper/proxy": "^4.4.1",
        "fzaninotto/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.2.0",
        "laravel/framework": "^8.14.0",
        "laravel/tinker": "^2.5.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.1.0",
        "phpunit/phpunit": "^9.4.3"
    },
    "autoload": {
        "psr-4": {
            "Zablose\\Rap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "laravel/app/",
            "Tests\\": "laravel/tests/"
        },
        "classmap": [
            "laravel/database/factories"
        ]
    }
}
